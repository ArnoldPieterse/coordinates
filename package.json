{
  "name": "rekursing-ai-gaming",
  "version": "1.0.0",
  "description": "Revolutionary multiplayer planetary shooter where AI systems recursively learn and improve from every player interaction, built with Three.js, mathematical physics integration, and recursive AI systems",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "start": "concurrently \"npx kill-port 3001 && npm run server\" \"npm run dev\"",
    "start:prod": "node server.js",
    "test": "node api-test-suite.js",
    "test:api": "node api-test-suite.js",
    "test:watch": "nodemon api-test-suite.js",
    "test:coverage": "echo \"No coverage tool configured\" && exit 0",
