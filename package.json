    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@lmstudio/sdk": "^0.1.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "three": "^0.158.0"
  },
