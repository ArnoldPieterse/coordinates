    "setup:aws": "node setup-aws-and-deploy.js",
    "setup:aws:agents": "node setup-aws-and-deploy.js --ai-agents",
    "aws:credentials": "node setup-aws-credentials.js",
    "aws:test": "node test-aws-simple.js",
    "ai:workflow": "node enhanced-ai-workflow.js",
    "ai:deploy": "node fix-s3-deployment.js",
    "deploy:build": "node deploy.js build",
    "deploy:help": "node deploy.js help",
    "domain:configure": "node aws-domain-config.js",
    "domain:verify": "nslookup www.rekursing.com",
    "deploy:aws-complete": "node deploy.js aws --prod",
    "deploy:aws-static": "node deploy.js aws",
    "deploy:aws-robust": "node deploy.js aws --prod",
    "deploy:aws-final": "node deploy.js aws --prod",
    "deploy:aws-auto": "node deploy.js aws --prod",
    "deploy:s3-simple": "node deploy.js aws",
    "deploy:aws-cli": "node deploy.js aws",
    "deploy:final": "node deploy.js aws --prod",
    "deploy:working": "node deploy.js aws",
    "deploy:rekursing": "node deploy-rekursing-complete.js",
    "deploy:rekursing:prod": "set NODE_ENV=production && node deploy-rekursing-complete.js",
    "setup:infrastructure": "node setup-complete-infrastructure.js",
    "deploy:full": "node deploy-full-project.cjs",
    "auto-git": "powershell -ExecutionPolicy Bypass -File ./auto-git.ps1",
    "gpu-streaming": "node launch-gpu-streaming.js",
    "gpu-streaming:dev": "concurrently \"npm run gpu-streaming\" \"npm run dev\"",
    "gpu-streaming:test": "node test-gpu-streaming.js",
    "gpu-streaming:demo": "node demo-gpu-streaming.js"
