    "deploy:production": "node deploy.js local --prod",
    "deploy:docker": "node deploy.js docker",
    "deploy:compose": "node deploy.js docker --compose",
    "deploy:heroku": "node deploy.js heroku",
    "deploy:vercel": "node deploy.js vercel",
    "deploy:netlify": "node deploy.js netlify",
    "deploy:aws": "node deploy.js aws",
    "deploy:aws-prod": "node deploy.js aws --prod",
    "domain:configure-modern": "node aws-domain-config-modern.js",
    "build:agents": "node build-with-agents.js",
